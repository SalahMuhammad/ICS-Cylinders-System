	<parameter name="Name" class="java.lang.String"/>
	<parameter name="CustomerID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     accessories.`id` AS accessories_id,
     accessories.`name` AS accessories_name,
     accessories_transactions.`id` AS accessories_transactions_id,
     accessories_transactions.`customer_id` AS accessories_transactions_customer_id,
     accessories_transactions.`accessor_id` AS accessories_transactions_accessor_id,
     accessories_transactions.`price` AS accessories_transactions_price,
     accessories_transactions.`count` AS accessories_transactions_count,
     accessories_transactions.`date` AS accessories_transactions_date
FROM
     `accessories` accessories INNER JOIN `accessories_transactions` accessories_transactions ON accessories.`id` = accessories_transactions.`accessor_id`
WHERE
     accessories_transactions.`customer_id` = $P{CustomerID}
ORDER BY
     accessories_transactions.`date` DESC]]>
	</queryString>
